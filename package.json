{
    "name": "multi-store-sync",
    "version": "1.0.0",
    "description": "تطبيق لمزامنة المخزون بين متاجر شوبيفاي المتعددة",
    "main": "server/server.js",
    "scripts": {
      "start": "node server/server.js",
      "dev:server": "nodemon server/server.js",
      "dev:client": "cd client && npm start",
      "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
      "install:all": "npm install && cd client && npm install",
      "build": "cd client && npm run build",
      "test": "jest"
    },
    "keywords": [
      "shopify",
      "sync",
      "inventory",
      "ecommerce"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
      "axios": "^1.6.2",
      "bcryptjs": "^2.4.3",
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "express-rate-limit": "^7.1.1",
      "helmet": "^7.0.0",
      "joi": "^17.11.0",
      "jsonwebtoken": "^9.0.2",
      "mongoose": "^7.6.3",
      "morgan": "^1.10.0",
      "node-cron": "^3.0.2",
      "pg": "^8.11.3",
      "winston": "^3.11.0"
    },
    "devDependencies": {
      "concurrently": "^8.2.2",
      "jest": "^29.7.0",
      "nodemon": "^3.0.1",
      "supertest": "^6.3.3"
    }
  }